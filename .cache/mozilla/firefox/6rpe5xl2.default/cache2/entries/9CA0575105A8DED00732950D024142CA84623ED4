						<script type="text/javascript">
			WP.$(function($) {
				function initSigninForm() {
					var
							login = $("#login").focus(),
							loginForm = $('#loginForm').attr('action', 'https://profil.wp.pl/login_poczta.html'),
							loginInfo = login.nextAll(".info"),
							password = $("#password"),
							passwordInfo = password.nextAll(".info"),
							btnSubmit = $("#btnSubmit"),
							validSignin = [];
					login.validate(validSignin, isOldLoginOrEmail, {
						onError: function() {
							loginInfo.html(isEmpty(login) ? EMPTY_LOGIN_EMAIL : INVALID_LOGIN);
							loginInfo.fadeIn();
						},
						onSuccess: function() {
							loginInfo.hide();
						}
					});

					password.validate(validSignin, isNotEmpty, {
						onError: function() {
							passwordInfo.text(isEmpty(password) ? EMPTY_PASSWORD : INVALID_PASSWORD);
							passwordInfo.fadeIn();
						},
						onSuccess: function() {
							passwordInfo.hide();
						}
					});
					btnSubmit.bind("click", function(e) {
						checkValidity(validSignin, true);
						if (!checkValidity(validSignin))
							e.preventDefault();
					});
				}
				initSigninForm();
			});
		</script>
